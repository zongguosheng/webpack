page{ background: #f5f5f5;}
swiper{ height: 750rpx;}
.swiper-item{ width: 750rpx; height: 750rpx;}
.swiper-item image{width: 750rpx; height: 750rpx;}

.discounttext{ background: #fff; padding: 32rpx;}
.disprice{ color: #ff6371; font-size: 24rpx; margin-bottom: 10px; display: flex; flex-direction: row; justify-content: space-between; align-items: center;}
.price{ color: #ff6371; font-size: 24rpx; }
.aftertext{ font-size: 48rpx; margin-right: 16rpx;}
.price .oldprice{text-decoration: line-through; color: #9e9e9e; font-size: 24rpx;}
.discounttext .afterprice{ color: #fff; background: #ffb4bb; font-size: 24rpx; display: inline-block; padding: 3px 10px; border-radius: 11px; margin-right: 10px;}
.ad{ display: flex; flex-direction: row; justify-content: space-between; align-items: center; margin-top: 20rpx;}
.ad text{ font-size: 24rpx; color: #9e9e9e;}
.goodsinfo{ margin-top: 10rpx; background: #fff; padding: 32rpx;}
.goodsinfo .row{display: flex; flex-direction: row; padding: 10rpx 0;}
.goodsinfo .row label{ font-size: 24rpx; color: #9e9e9e; margin-right: 30rpx;}
.goodsinfo .row text{ font-size: 24rpx; color: #292929; }
.icon34{ width: 34rpx; height: 34rpx;}
.scrollmodule{ background: #fff; margin-top: 10rpx; padding: 10px 10px;}
.scrollmodule .title{ font-size: 32rpx; color: #292929; padding: 10rpx; line-height: 48rpx; margin-bottom: 10px;}
.scroll-view_H{ height: 240rpx; width: 100%; white-space: nowrap;  background: #fff; }
.scroll-view-item_H{ width: 156rpx; display: inline-block; margin: 0 5px;}
.scroll-view-item_H .msimg{ width: 156rpx; height: 156rpx; border-radius: 5px;}
.scroll-view-item_H .price{ font-size: 32rpx; color: #ff6371; text-align: center;}
.scroll-view-item_H .price text{ font-size: 24rpx;}
.scroll-view-item_H .oldprice{ font-size: 24rpx; color: #c7c7c7; text-decoration: line-through; text-align: center;}
.scroll-view-item_H .oldprice text{ font-size: 24rpx;}







.coupon{ width: 686rpx; height: 178rpx; background: url(data:image/png;base64,); background-size: contain; margin-top: 40rpx; display: flex; flex-direction: row; align-items: center; justify-content: space-between; padding: 0 20px;}
.coupon .couponprice{ font-size: 24rpx; color: #292929; padding: 0 20rpx;}
.coupon .couponprice .red{ color: #f4094e; font-size: 48rpx;}
.coupon .couponprice .fs60{ color: #f4094e; font-size: 120rpx; margin-right: 30rpx;}

.coupon .fs36{ font-size: 36rpx; color: #fff; width: 90rpx; font-weight: bold;}