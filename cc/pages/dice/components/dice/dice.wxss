.dice-container .confirm-btn {
    display: flex;
    justify-content: center;
    align-self: center;
    width: 280rpx;
    height: 114rpx;
    background: linear-gradient(180deg,rgba(255,246,96,1),rgba(228,200,7,1));
    box-shadow: 0px 14rpx 6rpx 0px rgba(238,99,18,1);
    border-radius: 57rpx;
    margin-bottom: 40rpx;
    text-align: center;
    line-height: 114rpx;
    font-size: 36rpx;
    font-family: PingFangSC-Semibold;
    color: rgba(191,43,23,1);
}

.dice-container .shake-pic {
    align-self: center;
    width: 128rpx;
    height: 128rpx;
    margin-bottom: 40rpx;
}

.dice-container .master-open {
    align-self: center;
    display: flex;
    height: 288rpx;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 20rpx;
}

.dice-container .master-open .bubble {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 368rpx;
    height: 90rpx;
    padding: 0 36rpx;
    background: rgba(255,255,255,1);
    border-radius: 39rpx;
    font-family: AppleSystemUIFont;
    line-height: 84rpx;
    margin-bottom: 20rpx;
    position: relative;
}

.dice-container .master-open .open {
    display: flex;
    justify-content: center;
    align-items: center;
