@-webkit-keyframes wobble {
    from {
        transform: none;
    }

    1% {
        transform: translate3d(-20%,0,0) rotate3d(0,0,1,-5deg);
    }

    2% {
        transform: translate3d(20%,0,0) rotate3d(0,0,1,5deg);
    }

    4% {
        transform: none;
    }
}

@keyframes wobble {
    from {
        transform: none;
    }

    1% {
        transform: translate3d(-20%,0,0) rotate3d(0,0,1,-5deg);
    }

    2% {
        transform: translate3d(20%,0,0) rotate3d(0,0,1,5deg);
    }

    4% {
