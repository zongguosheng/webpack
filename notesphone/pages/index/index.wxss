.classify {
    margin-top: 1px;
    box-sizing: border-box;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    flex-wrap: nowrap;
    background: #fff;
}

.classify text{ height: 80rpx; line-height: 80rpx; font-size: 36rpx; }

.contentplan {
    box-sizing: border-box;
    width: 100%;
    background: #fff;
    margin-bottom: 30rpx;
    border-top: 1rpx solid #fff;
    border-bottom: 1rpx solid #EFEFF4;
}
.contenttitle {
    box-sizing: border-box;
    width: 100%;
    line-height: 100rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    flex-wrap: nowrap;
    padding-left: 40rpx;
    padding-right: 40rpx;
    font-size: 14px;
    color: #686868;
}

.contentrow {
    box-sizing: border-box;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    flex-wrap: nowrap;
}

.contentname {
    box-sizing: border-box;
    width: 100%;
    border-top: 1rpx solid #E6E7E8;
    line-height: 100rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    flex-wrap: nowrap;
    padding-right: 40rpx;
    color: #111;
}

.imgplan {
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    flex-wrap: nowrap;
}
.conimg {
    width: 48rpx;
    height: 48rpx;
    margin-right: 15rpx;
}
.routeimgclose {
    width: 45rpx;
    height: 45rpx;
    transform: rotate(-90deg);
}

.routeimgopen {
    width: 45rpx;
    height: 45rpx;
    transform: rotate(90deg);
}
.arrimg {
    width: 45rpx;
    height: 45rpx;
}
