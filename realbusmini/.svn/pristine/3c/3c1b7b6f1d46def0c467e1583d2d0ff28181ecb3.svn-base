    line-height: 84rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Medium;
    color: rgba(255,255,255,1);
}

.corner {
    position: absolute;
    left: 50%;
    top: 4rpx;
    padding: 0 10rpx;
    line-height: 28rpx;
    border-radius: 14rpx;
}

.game_enter_size {
    width: 750rpx;
    height: 340rpx;
}

.game_enter {
