.dice-container {
    display: flex;
    justify-content: flex-end;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    width: 680rpx;
    height: 1120rpx;
    background: rgba(255,255,255,0.1);
    border-radius: 60rpx;
    border: 8rpx solid rgba(255,255,255,0.2);
    margin: 40rpx 0 0 36rpx;
}

.dice {
    display: flex;
    width: 74rpx;
    height: 74rpx;
    background: rgba(255,255,255,1);
    box-shadow: 0px 2rpx 4rpx 0rpx rgba(0,0,0,0.1),0px 0rpx 5rpx 0rpx rgba(190,186,186,0.5);
    border-radius: 24rpx;
    padding: 15rpx;
    position: absolute;
    top: 5%;
    left: 10%;
    animation-duration: 1.5s;
    animation-fill-mode: both;
    animation-direction: alternate;
}

.dice .item {
    width: 20rpx;
    height: 20rpx;
    background-color: #000;
    border-radius: 100%;
    overflow: hidden;
}

.dice1 {
    justify-content: center;
    align-items: center;
}

.dice1 .item {
    width: 36rpx;
    height: 36rpx;
    background: rgba(254,2,61,1);
}

.dice2 {
    justify-content: space-between;
}

.dice2 .item:nth-child(2) {
    align-self: flex-end;
}

.dice3 {
    justify-content: space-between;
}

.dice3 .item:nth-child(2) {
    align-self: center;
}

.dice3 .item:nth-child(3) {
    align-self: flex-end;
}

.dice4 {
    flex-wrap: wrap;
    align-content: space-between;
}

.dice4 .row {
    flex-basis: 100%;
    display: flex;
    justify-content: space-between;
}

.dice4 .row .item {
    background: rgba(254,2,61,1);
}

.dice5 {
    flex-wrap: wrap;
    align-content: space-between;
}

.dice5 .row {
    flex-basis: 100%;
    display: flex;
    justify-content: space-between;
}

.dice5 .row:nth-child(2) {
    justify-content: center;
}

.dice6 {
    flex-wrap: wrap;
    align-content: space-between;
}

.dice6 .row {
    flex-basis: 100%;
    display: flex;
    justify-content: space-between;
}

