.container {
    margin-bottom: 8px;
}

.line-detail {
    height: 36px;
    padding: 12px 16px;
    font-size: 15px;
}

.line-runtime {
    margin-top: 5px;
}

.time-table-page {
}

.top {
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
    z-index: 2;
}

.top-bar {
    width: 100%;
    height: 60px;
    padding: 10px 15px;
}

.placeholder {
    width: 100%;
    height: 10px;
}

.table-container {
    margin-top: 10px;
}

.times-list {
    margin-bottom: 1px;
}

.title {
    width: 100%;
    height: 48px;
    position: relative;
    display: flex;
    align-items: center;
}

.time-table-ic {
    width: 12px;
    height: 12px;
    margin-right: 8px;
    margin-left: 15px;
}

.tag-type {
    border-radius: 4px;
    padding: 0 4px;
    height: 18px;
    background-color: rgba(255,90,0,0.08);
    border: solid 1px rgba(255,90,0,0.24);
    margin-left: 16px;
}

.content {
    width: 100%;
    padding: 15px 0 5px 0;
    text-align: center;
}

.block {
    width: 25%;
    height: 18px;
    margin-bottom: 10px;
    display: inline-block;
    text-align: center;
}

.time-text {
    position: relative;
}

.circle {
    position: absolute;
    left: 19px;
    top: 6px;
    width: 5px;
    height: 5px;
    border-radius: 50%;
}

.times-information {
    position: fixed;
    bottom: 0;
    left: 0;
    height: 48px;
    width: 100%;
}

.icon_timetable_open_rotate {
    transform: rotate(180deg);
}

.time-table-arrow {
    width: 12px;
    height: 7px;
    position: absolute;
    right: 16px;
    top: 20px;
}