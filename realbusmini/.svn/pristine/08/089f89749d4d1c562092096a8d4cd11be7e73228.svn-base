.modal {
    width: 270px;
    border-radius: 10px;
    height: 120px;
    position: fixed;
    z-index: 10001;
    top: 50%;
    margin-top: -60px;
    left: 50%;
    margin-left: -135px;
}

.modal1 {
    width: 270px;
    border-radius: 10px;
    height: 120px;
    position: fixed;
    z-index: 10001;
    top: 300rpx;
    left: 50%;
    margin-left: -135px;
}

.modal-column-split-line {
    width: 100%;
    height: 1px;
}

.modal-row-split-line {
    position: absolute;
    height: 43px;
    left: 50%;
    width: 1px;
}

.modal-title {
    height: 76px;
    flex-direction: column;
    position: relative;
}

.modal-btns {
    display: flex;
    position: relative;
}

.modal-btn {
    flex: 1;
    height: 43px;
}

.fav-tag-length {
    height: 20px;
    width: 24px;
    position: absolute;
    right: 16px;
    top: 50%;
    margin-top: -10px;
}

.modal-btns button {
    margin: 0;
    background-color: white;
    position: unset;
    border-radius: 10px;
    padding: 0;
}

.modal-btns button:after {
    border: none;
}