/* pages/classification/classification.wxss */
.header{height: 150px; display: flex; flex-direction: column; padding-top: 15px;}

.top{display: flex; flex-direction: row; justify-content: space-between; color: #fff; padding: 10px 15px; align-items: center;}
.weather{ font-size: 12px; line-height: 24px; }
.searchbar{padding:10px 15px;}
.placeholdercolor{ color: #b3b3b3;}
.tool text{ margin-left: 27px;}
.searchinput{display: flex; flex-direction: row; align-items: center; padding-left: 10px; background: #fff; border-radius: 21.5px; height: 43px; line-height: 43px;}
.searchinput icon{height: 20px; line-height: 43px; margin: 0 10px;}
.searchinput input{height: 43px; line-height: 43px;width: 100%}
.classlist{ display: flex; flex-flow: row wrap; width: 720rpx; margin: 0 auto}
.classlist view image{ width: 40px; height: 40px; margin: 0 auto;}
.classlist view {
    padding: 15px;
    display: flex;
    width: 25%;
    height: 25%;
    flex-direction: column;
    justify-content: center;
    text-align: center;
}
.classlist view text{text-align: center; font-size: 22rpx; color: #404040;}

.header{
  background-image: url('data:image/jpeg;base64,');
  background-size:cover;
}