.profile-box {
    height: 36px;
    padding: 12px 16px;
    font-size: 15px;
    position: relative;
    background-color: #fbfbfd;
}

.line-sn {
    padding-right: 54px;
    font-size: 12px;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.line-runtime {
    margin-top: 5px;
    font-size: 10px;
    color: #666;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}