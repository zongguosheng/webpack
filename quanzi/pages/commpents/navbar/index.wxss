/* pages/commpents/navbar/index.wxss */
.bgcolor{ background: #fff;}
.bgimg{ background: url(data:image/png;base64,); background-size: contain;}
.personinfo{ display: flex; flex-direction: row; align-items: center; padding: 10px 15px; padding-top: 90px;}
.personinfo .left{flex-grow: 1; margin-right: 15px;}
.personinfo .right{flex-grow: 2;}
.userinfo{ display: flex; flex-direction: row; justify-content: space-between; align-items: center; margin-bottom: 5px; height: 48rpx;}
.userinfo .username{ font-size: 32rpx;}
.userinfo .time{ font-size: 12px; color: #bfbfbf;}
.messagetext{ display: flex; flex-direction: row; justify-content:space-between;}
.messagetext text{font-size: 28rpx; line-height: 25px; color: #999; overflow: hidden; text-overflow: ellipsis; display: box; display: -webkit-box; line-clamp: 1; -webkit-line-clamp: 1; -webkit-box-orient: vertical; width: 80%;}
.messagetext label{ display: inline-block; width: 25px; height: 25px; background: #fa5050; color: #fff; font-size: 12px; line-height: 25px; border-radius: 50%; text-align: center;}
.personinfo .head{ width: 110rpx; height: 110rpx; border-radius: 50%; border: 1px solid #fff;}
.personinfo .head image{ width: 110rpx; height: 110rpx; border-radius: 50%;}
.icon12{ display: inline-block; width: 24rpx; height: 24rpx; line-height: 48rpx; }
.personinfo .username{ color: #fff;}
.personinfo navigator{ color: #fff; font-size: 24rpx; display: flex; align-items: center;}
.messagetext text{color: #fff;}
.headstyle .card{ background:url(data:image/png;base64,) no-repeat 0 15px ; background-size: cover;  height: 140rpx; border-radius: 8px 8px 0 0; margin: 0 20px; display: flex; justify-content: space-between; align-items: center;}
.card label{color: #694b17; font-size: 14px; display: inline-block; margin-top: 30px; line-height: 50px; padding: 0 20px;}

.navbar-title {
  width: 100%;
  box-sizing: border-box;
  padding-left: 15px;
  padding-right: 15px;
  height: 32px;
  line-height: 32px;
  text-align: left;
  position: absolute;
  left: 0;
  z-index: 10; 
  color: #333;
  font-size: 16px;
  font-weight: normal;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}