page {
    height: 100%;
    font-size: 14px;
    font-family: Regular;
    background-color: #f0f0f5;
}

.ico {
    display: inline-block;
    vertical-align: middle;
    cursor: pointer;
}

.text-ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.text-right {
    text-align: right;
}

.text-center {
    text-align: center;
}

.refresh-pub {
    width: 48px;
    height: 48px;
    align-items: center;
    bottom: 65px;
    position: fixed;
    z-index: 4;
    background-color: #0082dc;
    box-shadow: 0 0 1px #0082dc;
    left: 50%;
    margin-left: -24px;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    -webkit-box-align: center;
}

.refresh-pub-iphone-x {
    bottom: 99px;
}

.iphone-x-bottom-fix {
    padding-bottom: 34px;
}

