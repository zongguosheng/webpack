/* pages/classification/classification.wxss */
.header{height: 150px; display: flex; flex-direction: column; padding-top: 15px;}
.userinfo{display: flex; flex-direction: row; align-items: center;}
.username{display: flex; flex-direction: row; align-items: center;}
.usertext{ margin-left: 10px; display: flex; flex-direction: column;}
.usertext .nicheng{line-height: 28px; font-size: 32rpx;}
.usertext .span{ background: #a23e47; text-align: center; display: inline-block; width: 60px; font-size: 24rpx; border-radius: 5px; padding: 2px 0;}
.userinfo image{width: 105rpx; height: 105rpx; border-radius: 50%; border: 3px solid #fff;}
.top{display: flex; flex-direction: row; justify-content: space-between; color: #fff; padding: 10px 15px; align-items: center;}
.weather{ font-size: 12px; line-height: 24px; }
.searchbar{padding:2px 15px;}
.placeholdercolor{ color: #b3b3b3;}
.tool text{ margin-left: 27px;}
.tool view{ font-size: 32rpx;}
.searchinput{display: flex; flex-direction: row; align-items: center; padding-left: 10px; background: #fff; border-radius: 21.5px; height: 43px; line-height: 43px;}
.searchinput icon{height: 20px; line-height: 43px; margin: 0 10px;}
.searchinput input{height: 43px; line-height: 43px;width: 100%}
.leftflex{display: flex; flex-direction: row; justify-content: space-between; align-items: center;}
.classtitle{display: flex; flex-direction: row; justify-content: space-between; align-items: center; padding: 10px 0; margin: 0 10px; border-bottom: 1px solid #e6e6e6;}
.classtitle image{ width: 114rpx; height: 28rpx; margin-left: 10px; margin-right: 15px; }
.classtitle .titletext{font-size: 24rpx; color: #333; line-height: 26px;}
.classtitle .iconfont{color: #d5d5d5;}
.classlist{ display: flex; flex-flow: row wrap; width: 720rpx; margin: 0 auto}
.classlist view image{ width: 40px; height: 40px; margin: 0 auto;}
.classlist view {
    padding: 15px;
    display: flex;
    width: 25%;
    height: 25%;
    flex-direction: column;
    justify-content: center;
    text-align: center;
}
.classlist view text{text-align: center; font-size: 22rpx; color: #404040;}

.nav-tab{ display: flex; flex-direction: row; padding: 10px 10px 0 10px; border-top: 3px solid #f5f5f5;}
.nav-tab .nav-title{ font-size: 32rpx; margin: 0 10px; color: #b2b2b2; position: relative;}
.nav-tab .nav-title.active{color: #333; font-weight: bold;}
.nav-tab .nav-title.active text{
  position: absolute;
  left: 0;
  bottom: 0;
  margin-left: -5px; 
  padding:0 5px;
  display: inline-block; 
  height: 8px; width: 100%;
  border-radius: 4px;
  background: #ffe0e3;
  z-index: -1
}

.contentrow {
  padding: 0 20px 0 10px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: nowrap;
}

.contentname {
  width: 100%;
  line-height: 50rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  flex-wrap: nowrap;
  padding: 10px;
  border-bottom: 1rpx solid #E6E7E8;
  color: #111;
}

.imgplan {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: nowrap;
}
.imgplan image{ width: 160rpx; height: 120rpx; margin-right: 10px; border-radius: 5px;}
.servertitle text{
  height: 28px;
  font-size: 16px;
  color: #333;
  font-weight: 600;
  line-height: 28px;
  text-overflow:ellipsis;
  overflow:hidden;
  display:-webkit-box;
  -webkit-line-clamp:1; /*这个数字是设置要显示省略号的行数*/
  -webkit-box-orient:vertical;
}
.detailinfor {
  margin-right: 40rpx;
  font-size: 28rpx;
  color: #b3b3b3;
}
.inforcon {
  width: 380rpx;
  padding-top: 15rpx;
  padding-bottom: 10rpx;
}
.telimg{ 
  display: block;
  background: #ff6371; 
  border: 1px solid #ff6371; 
  border-radius: 50%; 
  text-align: center;
  width: 42px; 
  height: 42px; 
  line-height: 42px; 
  color: #fff; 
  font-size: 24px;
}
.show{
  display: block;
}
.hidden{
  display: none;
}
.header{
  background-image: url('data:image/jpeg;base64,');
  background-size:cover;
}

@media screen and (max-width: 320px) {
    .inforcon {width: 350rpx;}
}