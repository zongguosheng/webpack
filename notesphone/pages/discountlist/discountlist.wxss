.discountlist{ display: flex; flex-flow: row wrap; padding: 10px;}
.discountlist .li{ width: 334rpx; margin: 30rpx 12rpx 0rpx 12rpx; }
.discountlist .li .picture{ position: relative; width: 334rpx; border-radius: 5px;}
.discountlist .li .picture image{ width: 334rpx; height: 334rpx;}
.discountlist .li .describe{overflow: hidden; text-overflow: ellipsis; display: box; display: -webkit-box; line-clamp: 1; -webkit-line-clamp: 1; -webkit-box-orient: vertical; color: #292929; font-size: 24rpx; line-height: 50rpx;
}
.discountlist .li .afterprice{ display: flex; flex-direction: row; justify-content: space-between; align-items: center;}
.afterprice .price{color: #ff6371; font-size: 24rpx;}
.afterprice .price text{ font-size: 32rpx;}
.afterprice .price label{ color: #9e9e9e;}
.afterprice .ticket{ color: #fff; background: #ff6371; border-radius: 36rpx; height: 36rpx; padding: 0 10px; font-size: 24rpx;}