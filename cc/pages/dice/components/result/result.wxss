.result-container {
    display: flex;
    position: fixed;
    left: 30rpx;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    height: 100%;
    padding: 40rpx 0 64rpx 0;
    box-sizing: border-box;
}

.result-panel .result-title {
    display: flex;
    flex-direction: column;
    width: 690rpx;
    height: 280rpx;
    background: rgba(247,245,219,1);
    border-radius: 16rpx;
    margin-bottom: 20rpx;
}

.result-panel .result-title .orgin {
    display: flex;
    height: 120rpx;
    justify-content: space-between;
    line-height: 120rpx;
    border-bottom: 2rpx solid #e9e7cb;
    font-size: 28rpx;
    color: rgba(191,43,23,1);
    width: 100%;
    padding: 0 20rpx;
    box-sizing: border-box;
}

.result-panel .result-title .orgin .title {
    width: 130rpx;
}

.result-panel .result-title .orgin .content {
    display: flex;
    justify-content: space-between;
    width: 100%;
    text-align: center;
}

.result-panel .result-title .plus {
    display: flex;
    justify-content: space-between;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 2rpx solid #e9e7cb;
    background: rgba(251,251,243,1);
    font-size: 28rpx;
    color: rgba(191,43,23,1);
    width: 100%;
    padding: 0 20rpx;
    box-sizing: border-box;
}

.result-panel .result-title .plus .title {
    width: 130rpx;
}

.result-panel .result-title .plus .content {
    display: flex;
    justify-content: space-between;
    width: 100%;
    text-align: center;
    padding: 0 15rpx 0 10rpx;
    box-sizing: border-box;
}

.result-panel .result-title .one {
    display: flex;
    justify-content: space-between;
    background: rgba(251,251,243,1);
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 16rpx;
    font-size: 28rpx;
    color: rgba(191,43,23,1);
    width: 100%;
    padding: 0 20rpx;
    box-sizing: border-box;
}

.result-panel .result-title .one .title {
    width: 130rpx;
}

.result-panel .result-title .one .content {
    display: flex;
    justify-content: space-between;
    width: 100%;
    text-align: center;
    padding: 0 15rpx 0 10rpx;
    box-sizing: border-box;
}

.result-panel .result-title .one .content .num {
    width: 100%;
}

.result-panel .result-dice {
    display: flex;
    flex-direction: column;
    width: 690rpx;
    background: rgba(247,245,219,1);
    border-radius: 16rpx;
    overflow: hidden;
}

.result-panel .result-dice .dice-show {
    display: flex;
    height: 156rpx;
    border-bottom: 4rpx solid #e9e7cb;
    padding: 30rpx 0;
    box-sizing: border-box;
}

.result-panel .result-dice .dice-show .header {
    width: 220rpx;
    padding: 0 20rpx 0 30rpx;
    display: flex;
    justify-content: flex-start;
}

.result-panel .result-dice .dice-show .header .pics {
    width: 100rpx;
    height: 100rpx;
    margin-right: 24rpx;
    box-shadow: 0px 2rpx 4rpx 0px rgba(0,0,0,0.1);
    border-radius: 16rpx;
}

.result-panel .result-dice .dice-show .header .head-info {
    display: flex;
    flex-direction: column;
    font-size: 28rpx;
    font-family: PingFangSC-Medium;
    color: rgba(191,43,23,1);
    line-height: 40rpx;
    padding-top: 10rpx;
}

.result-panel .result-dice .dice-show .header .head-info .name,.result-panel .result-dice .dice-show .header .head-info .time {
    width: 108rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.result-panel .result-dice .dice-show .container {
    display: flex;
    align-items: center;
    padding-right: 15rpx;
}

.dice {
    display: flex;
    width: 62rpx;
    height: 62rpx;
    background: rgba(255,255,255,1);
    box-shadow: 0px 2rpx 4rpx 0rpx rgba(0,0,0,0.1),0px 0rpx 5rpx 0rpx rgba(190,186,186,0.5);
    border-radius: 12rpx;
    padding: 10rpx;
    box-sizing: border-box;
    margin-right: 8rpx;
}

.dice .item {
    width: 12rpx;
    height: 12rpx;
    background: rgba(46,46,46,1);
    border-radius: 100%;
    overflow: hidden;
}

.dice1 {
    justify-content: center;
    align-items: center;
}

.dice1 .item {
    width: 21rpx;
    height: 21rpx;
    background: rgba(254,2,61,1);
}

.dice2 {
    justify-content: space-between;
}

.dice2 .item:nth-child(2) {
    align-self: flex-end;
}

.dice3 {
    justify-content: space-between;
}

.dice3 .item:nth-child(2) {
    align-self: center;
}

.dice3 .item:nth-child(3) {
    align-self: flex-end;
}

.dice4 {
    flex-wrap: wrap;
    align-content: space-between;
}

.dice4 .row {
    flex-basis: 100%;
    display: flex;
    justify-content: space-between;
}

.dice4 .row .item {
    background: rgba(254,2,61,1);
}

.dice5 {
    flex-wrap: wrap;
    align-content: space-between;
}

.dice5 .row {
    flex-basis: 100%;
    display: flex;
    justify-content: space-between;
}

.dice5 .row:nth-child(2) {
    justify-content: center;
}

.dice6 {
    flex-wrap: wrap;
    align-content: space-between;
}

.dice6 .row {
    flex-basis: 100%;
    display: flex;
    justify-content: space-between;
}

.result-container .result-option {
    align-self: flex-end;
    display: flex;
    justify-content: space-around;
    width: 100%;
    padding: 0 60rpx;
    box-sizing: border-box;
}

.result-container .result-option .confirm-exit {
    width: 280rpx;
    height: 114rpx;
    background: linear-gradient(180deg,rgba(255,246,96,1),rgba(228,200,7,1));
    box-shadow: 0px 14rpx 6rpx 0px rgba(238,99,18,1);
    border-radius: 57rpx;
    text-align: center;
    font-size: 36rpx;
    font-family: PingFangSC-Semibold;
    color: rgba(191,43,23,1);
    line-height: 114rpx;
}

.result-container .result-option .confirm-replay {
    width: 280rpx;
    height: 114rpx;
    background: linear-gradient(180deg,rgba(255,246,96,1),rgba(228,200,7,1));
    box-shadow: 0px 14rpx 6rpx 0px rgba(238,99,18,1);
    border-radius: 57rpx;
    text-align: center;
    font-size: 36rpx;
    font-family: PingFangSC-Semibold;
    color: rgba(191,43,23,1);
    line-height: 114rpx;
}