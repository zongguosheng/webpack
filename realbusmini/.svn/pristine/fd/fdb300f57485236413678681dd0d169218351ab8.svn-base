.collect-bubble-tip {
    position: absolute;
    bottom: 40px;
    right: -27rpx;
    width: 416rpx;
    height: 56px;
    border-radius: 8px;
    box-sizing: border-box;
    box-shadow: 0 6px 12px 0 rgba(0,0,0,0.16);
    z-index: 5;
    background: -webkit-linear-gradient(left,#2294e5,#57b4f6);
}

.text-left {
    text-align: left;
}

.collect-bubble-content {
    width: 116px;
}

.collect-close-icon {
    width: 24px;
    height: 22.4px;
    position: absolute;
    right: 6px;
    top: 17px;
    z-index: 1;
}

.collect-bubble-bus {
    width: 120rpx;
    height: 100rpx;
    margin: 3px 8px;
}

.collect-down-arraw {
    position: absolute;
    bottom: -5px;
    right: 46rpx;
    width: 0px;
    height: 0px;
    border-top: 7px solid #4faff4;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    z-index: 0;
}

.fav-inner-box {
    width: 30px;
    height: 30px;
}

.no-can-swap.collect-bubble-tip {
    right: -55rpx;
}

.no-can-swap.collect-down-arraw {
    right: 72rpx;
}