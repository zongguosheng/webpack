page {
    background: #fff;
}

.container {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    font-family: PingFangSC-Medium;
}

.icon {
    width: 434rpx;
    height: 538rpx;
    margin: 52rpx auto 40rpx;
}

.title {
    height: 50rpx;
    font-size: 36rpx;
    font-weight: 500;
    line-height: 50rpx;
    margin-bottom: 40rpx;
    color: rgba(51,51,51,1);
}

.sub_title {
    font-size: 28rpx;
    font-weight: 400;
    line-height: 44rpx;
    width: 560rpx;
    margin-bottom: 24rpx;
    font-family: PingFangSC-Regular;
    color: rgba(102,102,102,1);
}

.tips {
    font-size: 32rpx;
    font-weight: 400;
    height: 40rpx;
    line-height: 40rpx;
    margin: 40rpx auto 24rpx;
    text-align: center;
    width: 100%;
    font-family: PingFangSC-Regular;
    color: rgba(255,90,0,1);
}

.btn {
    width: 264rpx;
    height: 84rpx;
    background: rgba(1,152,241,1);
    box-shadow: 0px 4px 8px 0px rgba(1,152,241,0.3);
    border-radius: 42rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(255,255,255,1);
    line-height: 84rpx;
}