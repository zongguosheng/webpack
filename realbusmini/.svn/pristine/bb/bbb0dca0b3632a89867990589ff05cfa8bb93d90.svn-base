<template name="searchBubble">
    <view catchtap="emptyEvent" class="search-bubble-tip fg-white f-14 relative flex-column-center lineDetailStyel" wx:if="{{lineDetailFlag}}">
        <view class="search-up-arraw"></view>
        <image class="search-bubble-bus" src="../../images/lead_time.png"></image>
        <view style="flex: 1">公交到站的时间</view>
        <image catchtap="closeSearchBubble" class="search-close-icon" src="../../images/close_white_btn@2x.png"></image>
    </view>
    <view class="search-bubble-tip fg-white f-14 relative flex-column-center lineDetailPayStyel" wx:if="{{payText.length}}">
        <view class="search-bottom-arraw"></view>
        <view style="flex: 1">{{payText}}</view>
        <image catchtap="closePayBubbleIn" class="search-close-icon" src="../../images/close_white_btn@2x.png"></image>
    </view>
</template>



